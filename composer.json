{
  "name": "dibukeu/client-php",
  "description": "dibuk client php library",
  "license": "MIT",
  "config": {
    "vendor-dir": "vendor",
    "platform": {
      "php": "7.0.17"
    }
  },
  "keywords": ["dibuk"],
  "authors": [
    {
      "name": "Dibuk",
      "email": "info@dibuk.sk"
    }
  ],
  "require": {
    "php": ">=5.4",
    "composer/ca-bundle": "~1.1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "Dibuk\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dibuk\\Test\\": "test"
    }
  }
}
